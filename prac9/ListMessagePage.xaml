<Page x:Class="prac9.ListMessagePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:prac9"
      mc:Ignorable="d" 
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ListMessagePage">

    <Grid>
        <ListBox x:Name="Messages" SelectionChanged="ListBox_SelectionChanged"/>
        <ProgressBar x:Name="Go"  IsIndeterminate="True" Visibility="Hidden"/>
        <Frame NavigationUIVisibility="Hidden" x:Name="framik"/>
    </Grid>
</Page>
